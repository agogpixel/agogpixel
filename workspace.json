{
  "version": 1,
  "projects": {
    "shared-phaser-ng-phaser": {
      "projectType": "library",
      "root": "libs/shared/phaser/ng-phaser",
      "sourceRoot": "libs/shared/phaser/ng-phaser/src",
      "prefix": "agogpixel",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/shared/phaser/ng-phaser/tsconfig.lib.json",
            "project": "libs/shared/phaser/ng-phaser/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/phaser/ng-phaser/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/phaser/ng-phaser/src/**/*.ts",
              "libs/shared/phaser/ng-phaser/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/shared/phaser/ng-phaser/src/test.ts",
            "tsConfig": "libs/shared/phaser/ng-phaser/tsconfig.spec.json",
            "karmaConfig": "libs/shared/phaser/ng-phaser/karma.conf.js"
          }
        }
      }
    },
    "workspace": {
      "root": "libs/workspace",
      "sourceRoot": "libs/workspace/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/workspace/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workspace"],
          "options": {
            "jestConfig": "libs/workspace/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/workspace",
            "tsConfig": "libs/workspace/tsconfig.lib.json",
            "packageJson": "libs/workspace/package.json",
            "main": "libs/workspace/src/index.ts",
            "assets": ["libs/workspace/*.md"]
          }
        }
      }
    },
    "gam": {
      "root": "libs/gam",
      "sourceRoot": "libs/gam/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/gam/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/gam"],
          "options": {
            "jestConfig": "libs/gam/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/gam",
            "tsConfig": "libs/gam/tsconfig.lib.json",
            "packageJson": "libs/gam/package.json",
            "main": "libs/gam/src/index.ts",
            "assets": ["libs/gam/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "karma"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "karma",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
